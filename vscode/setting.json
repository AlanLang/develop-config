{
  "workbench.colorTheme": "Ocean",
  "workbench.preferredDarkColorTheme": "Ocean",
  "workbench.preferredLightColorTheme": "Ocean",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.iconTheme": "file-icons",
  "workbench.fontAliasing": "antialiased",
  "editor.fontFamily": "'Fira Code', 'Monaco NF', 'Menlo', 'DejaVu Sans Mono', 'Lucida Console', monospace",
  "editor.fontSize": 14,
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss06', 'zero'",
  "editor.accessibilitySupport": "off",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "on",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "boundary",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.stickyScroll.enabled": true,
  "editor.hover.sticky": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true,
    "source.fixAll.stylelint": true,
    "source.fixAll.eslint": true,
    "source.fixAll": false,
    "source.organizeImports": false
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "window.autoDetectColorScheme": true,
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "custom",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabCloseButton": "left",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 5,
  "workbench.list.smoothScrolling": true,
  "workbench.sideBar.location": "right",
  "workbench.tree.expandMode": "singleClick",
  "workbench.tree.indent": 10,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": true,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.untrackedChanges": "separate",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontWeight": "300",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.fontFamily": "Monaco NF",
  "scm.diffDecorationsGutterWidth": 2,
  "debug.onTaskErrors": "debugAnyway",
  "diffEditor.ignoreTrimWhitespace": false,
  "search.exclude": {
    "**/.git": true,
    "**/.github": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
    "**/yarn.lock": true
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "gitlens.codeLens.enabled": false,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true,
    "typescript": true
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "GitCommitPlugin.Templates": [
    {
      "templateContent": "<type>:<space><subject><space>#<footer>",
      "default": true,
      "templateName": "fr"
    }
  ],
  "gauge.recommendedSettings.options": "Apply & Reload",
  "eslint.options": {
    "extensions": [".ts", ".html"]
  },
  "files.exclude": {
    "**/.project": true,
    "**/.settings": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "node_modules/": true
  },
  "security.workspace.trust.untrustedFiles": "open", 
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html"
  ],
  "GitCommitPlugin.ShowEmoji": false,
  "cSpell.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "editor.unicodeHighlight.allowedLocales": {
    "zh-hans": true
  },
  "editor.wordWrap": "on",
  "cSpell.userWords": [
    "alanlang",
    "antd",
    "bindgen",
    "Cmen",
    "craco",
    "draggable",
    "Dribbble",
    "glidejs",
    "Groupable",
    "linkding",
    "parms",
    "queryselector",
    "selecto",
    "sider",
    "snappable",
    "Submenu",
    "tanasuo",
    "Unmount"
  ],
  "files.autoSave": "off",
  "liveServer.settings.donotShowInfoMsg": true, 
  "workbench.editor.untitled.hint": "hidden",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "git.openRepositoryInParentFolders": "never"
}
